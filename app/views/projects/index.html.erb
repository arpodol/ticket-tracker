<h3>Projects</h3>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Open Tickets</th>
      <th scope="col">Total Tickets</th>
    </tr>
  </thead>
  <tbody>
    <% Project.all.each do |project| %>
      <%= render 'project', project:project %>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Project', new_project_path %>
