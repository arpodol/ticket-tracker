<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Assignee</th>
      <th scope="col">Status</th>
      <th scope="col">Tags</th>
    </tr>
  </thead>
  <tbody>
    <% tickets.each do |ticket| %>
    <tr>
      <th scope="row">
        <%=link_to ticket.name, ticket_path(ticket)%>
      </th>
      <td>TODO</td>
      <td><%=ticket.status%></td>
      <td>TODO</td>
    </tr>
    <% end %>
  </tbody>
</table>
